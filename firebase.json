{
  "hosting": [
    {
      "target": "inhpricelistgenerator",
      "public": ".",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "backup_*/**",
        "oldfiles/**",
        "quotemaker/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
